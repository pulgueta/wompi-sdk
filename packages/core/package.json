{
  "name": "@pulgueta/wompi",
  "version": "0.0.3",
  "description": "El SDK de Wompi para Node.js",
  "author": "Andrés Rodríguez <roariasaf@gmail.com>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pulgueta/wompi-sdk"
  },
  "bugs": {
    "url": "https://github.com/pulgueta/wompi-sdk/issues"
  },
  "homepage": "https://github.com/pulgueta/wompi-sdk",
  "files": ["dist"],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "release": "pnpm publish --no-git-check"
  },
  "keywords": [
    "wompi",
    "sdk",
    "wompi-sdk",
    "wompi-node-sdk",
    "bancolombia",
    "pagos",
    "pagos-bancolombia"
  ],
  "devDependencies": {
    "@pulgueta/wompi-tsconfig": "workspace:*",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.3.4",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/node": "^22.5.5",
    "@vitest/coverage-v8": "^2.1.1",
    "semantic-release": "^24.1.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  }
}
