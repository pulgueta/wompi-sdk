{
  "name": "@pulgueta/wompi",
  "version": "0.0.0",
  "description": "El SDK de Wompi para Node.js",
  "author": "Andrés Rodríguez <roariasaf@gmail.com>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pulgueta/wompi-sdk"
  },
  "bugs": {
    "url": "https://github.com/pulgueta/wompi-sdk/issues"
  },
  "homepage": "https://github.com/pulgueta/wompi-sdk",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "release": "pnpm publish --no-git-check"
  },
  "keywords": [
    "wompi",
    "sdk",
    "wompi-sdk",
    "wompi-node-sdk",
    "bancolombia",
    "pagos",
    "pagos-bancolombia"
  ],
  "devDependencies": {
    "@pulgueta/wompi-tsconfig": "workspace:*",
    "@types/node": "^22.5.5",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2"
  }
}
